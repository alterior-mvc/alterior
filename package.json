{
  "name": "@alterior/root-package",
  "version": "3.13.3",
  "private": true,
  "description": "An Express-based Typescript MVC framework with decorators and dependency injection.",
  "author": "The Alterior Project (https://github.com/alterior-mvc)",
  "license": "MIT",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/alterior-mvc/alterior.git"
  },
  "bugs": {
    "url": "https://github.com/alterior-mvc/alterior/issues"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "alt-build test",
    "bump": "alt-build version",
    "publish": "alt-build publish",
    "bootstrap": "ts-node scripts/bootstrap",
    "clean:npm:lock": "rimraf packages/*/package-lock.json",
    "clean:npm:modules": "rimraf packages/*/node_modules",
    "clean:npm": "npm run clean:npm:lock && npm run clean:npm:modules",
    "clean": "rimraf packages/*/dist packages/*/dist.esm",
    "build": "alt-build",
    "docs": "alt-build run docs --parallel"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.15",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^20.19.30",
    "@types/rimraf": "^4.0.5",
    "@types/serve-static": "1.15.0",
    "@types/supertest": "^2.0.9",
    "chai": "^4.1.2",
    "dotenv": "^16.3.1",
    "express": "^4.17.1",
    "kind-of": ">=6.0.3",
    "lodash": ">=4.17.19",
    "mkdirp": "^0.5.5",
    "razmin": "^1.2.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^6.1.2",
    "source-map-support": "^0.4.6",
    "supertest": "^3.1.0",
    "swagger-editor-dist": "^3.9.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.3.1",
    "typedoc": "^0.28.16",
    "typescript": "^5.9.3",
    "zone.js": "^0.14.3"
  },
  "dependencies": {
    "uuid": "^8.3.2"
  }
}
